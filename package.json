{
  "name": "nlp-qg",
  "version": "1.0.0",
  "description": "Global CLI for generating quiz questions from text and documents using multiple AI providers (Gemini, OpenAI, Anthropic, DeepSeek)",
  "main": "server.js",
  "bin": {
    "nlp-qg": "./cli/cli.js"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "setup": "node setup.js",
    "config": "node cli/cli.js config",
    "test": "node cli/cli.js test",
    "cli": "node cli/cli.js",
    "generate-key": "node utils/generateApiKey.js",
    "setup:python": "services/python/venv/bin/pip install -r services/python/requirements.txt",
    "build": "echo 'No build step required'"
  },
  "keywords": [
    "nlp",
    "gemini",
    "quiz",
    "question-generation"
  ],
  "author": "",
  "license": "GPL-3.0",
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "@scalar/express-api-reference": "^0.8.24",
    "adm-zip": "^0.5.16",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.5",
    "mammoth": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "officeparser": "^4.1.1",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^5.4.530",
    "pngjs": "^7.0.0",
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "node-fetch": "^2.7.0"
  }
}
